  color: #daa300;
  margin: 40rpx 28rpx;
  padding: 30rpx 30rpx 30rpx 10rpx;
  display: flex;
 align-items: center;
 font-weight: bold;
}
.notice-centent{
  margin-left: 20rpx;
}

.notice-fire {
  flex-shrink: 0;
  width: 80rpx;
  height: 80rpx;
}

/* 激励视频按钮 */
.detail-btn {
  display: flex;
  justify-content: center;
  align-items: center;
  background-image: linear-gradient(to right, #00c6fb 0%, #005bea 100%);
  border-radius: 200rpx;
  width: 80%;
