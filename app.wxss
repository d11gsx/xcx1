  margin: 140rpx auto 30rpx auto;
  color: #FFFFFF;
  font-weight: bold;
}

.detail-icon {
  height: 46rpx;
  width: 56rpx;
  margin-right: 20rpx;
  font-size: 28rpx;
}

.detail-no {
  text-align: center;
  color: rgb(165, 165, 165);
  margin-bottom: 60rpx;
}

/* 壁纸列表 */
.wall-video {
  height: 100%;
  width: 100%;
}

.wall-video-icon {
  height: 52rpx;
  width: 68rpx;
  overflow: hidden;
  position: absolute;
  bottom: 20rpx;
  left: 20rpx;
  z-index: 99;
}

.wall-lists-flex {
  display: flex;
  padding: 28rpx;
  justify-content: space-between;
  flex-wrap: wrap;
}

.wall-lists-item {
  border-radius: 20rpx;
  /* background-color: #f9f9f9; */
  width: 100%;
  flex-shrink: 0;
  margin-bottom: 16rpx;
  position: relative;
 break-inside: avoid; 
  -webkit-column-break-inside: avoid;
  -moz-column-break-inside: avoid;
}

.lwall-ists-title {
  margin: 20rpx;
}

.wall-lists-hot {
  display: inline;
  font-size: 28rpx;

}

.wall-lists-img {
  margin-bottom: 10rpx;
  overflow: hidden;
  position: relative;
}

.container {
  display: grid;
  grid-template-columns: 1fr 1fr;
  grid-template-rows: minmax(0, 100%)
}

/* 资源，文档列表 */
.lists-flex {
  display: flex;
  padding: 28rpx 40rpx;
  justify-content: space-between;
  flex-wrap: wrap;
}
.lists-page{
  text-align: center;
  padding: 30rpx 0;
  font-size: 26rpx;
  color: #d0d0d0;
  letter-spacing: 20rpx;
}
.lists-item {
  border-radius: 20rpx;
  padding: 10rpx;
  margin-bottom: 20rpx;
  position: relative;
 break-inside: avoid; 
  -webkit-column-break-inside: avoid;
  -moz-column-break-inside: avoid;
  background-color: #f9f9f9;
  /* overflow: hidden; */
}

.lists-title {
  margin: 0 10rpx 0 10rpx;
  font-size: 30rpx;
}

.lists-time {
  bottom: 10rpx;
  font-size: 21rpx;
  color: #fff !important;
  padding: 6rpx 13rpx;
  right: 0rpx !important;
  background-color: rgb(0 0 0 / 0.3);
  border-radius: 10rpx;
}

.lsits-tips {
  width: 100rpx;
  height: 70rpx;
  filter: drop-shadow(0 4rpx 6rpx rgba(0, 0, 0, 0.5));
  position: absolute;
  right: -10rpx;
  top: 10rpx;
  z-index: 9999;
  transform: rotate(-45deg);
  animation: maskTip 2s linear infinite;
}

@keyframes maskTip {
  0% {
    transform: rotate(-45deg);
  }

  50% {
    transform: rotate(0deg);
  }

  to {
    transform: rotate(-45deg);
  }
}

.lists-hot {
  display: inline;
  font-size: 28rpx;
}

.lists-img {
  /* height: 260rpx; */
  width: 100%;
  border-radius: 20rpx;
  margin-bottom: 10rpx;
  overflow: hidden;
  position: relative;
}

