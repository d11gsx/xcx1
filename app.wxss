.msak-3 {
  animation: maskRun3 8s linear infinite;
  background: radial-gradient(50% 50% at 50% 50%, rgba(7, 193, 96, .3) 0, rgba(7, 193, 96, 0) 100%);
}

.msak-2 {
  animation: maskRun2 14s linear infinite;
  background: radial-gradient(50% 50% at 50% 50%, rgba(255, 69, 79, .15) 0, rgba(255, 69, 79, 0) 100%);
}

.msak-1 {
  animation: maskRun1 10s linear infinite;
  background: radial-gradient(50% 50% at 50% 50%, rgba(255, 189, 46, .2) 0, rgba(255, 189, 46, 0) 100%);
}

.mask {
  border-radius: 100%;
  filter: blur(50px);
  height: 500px;
  left: 0;
  pointer-events: none;
  position: absolute;
  top: 0;
  width: 500px;
  z-index: 10;
}

@keyframes maskRun1 {
  0% {
    transform: translate3d(-25%, -25%, 0)
  }

  25% {
    transform: translate3d(0, 25%, 0)
  }

  50% {
    transform: translate3d(60%, 25%, 0)
  }

  75% {
    transform: translate3d(60%, -25%, 0)
  }

  to {
    transform: translate3d(-25%, -25%, 0)
  }
}

@keyframes maskRun2 {
  0% {
    transform: translate3d(-25%, -25%, 0)
  }

  25% {
    transform: translate3d(0, 25%, 0)
  }

  75% {
    transform: translate3d(60%, 25%, 0)
  }

  50% {
    transform: translate3d(60%, -25%, 0)
  }

  to {
    transform: translate3d(-25%, -25%, 0)
  }
}

@keyframes maskRun3 {
  0% {
    transform: translate3d(-25%, -25%, 0)
  }

  25% {
    transform: translate3d(60%, -25%, 0)
  }

  50% {
    transform: translate3d(0, 25%, 0)
  }

  75% {
    transform: translate3d(60%, 25%, 0)
  }

  to {
    transform: translate3d(-25%, -25%, 0)
  }
}

