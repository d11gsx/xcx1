/* 一言列表 */
.swiper-speench,
.swiper-speench-item {
  height: 700rpx;
}

.swiper-speench-list {
  background-color: #ffffff;
  box-shadow: #dadada 0px 5px 19px;
  width: 94%;
  height: 90%;
  margin: 5% auto 0 auto;
  border-radius: 40rpx;
  position: relative;
  overflow: hidden;
}

.speech-view {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 80%;
  transform: translate(-50%, -50%);

  perspective: 1000rpx;
  transition: all 1s;
  border-radius: 10px;
  cursor: pointer;
}

.speech-title {
  font-size: 80rpx;
  font-weight: bold;
  text-align: center;
}

.speech-cent {
  line-height: 60rpx;
  margin-top: 40rpx;
}

.spench-from {
  color: #d0d0d0;
  text-align: right;
  font-size: 28rpx;
}

.speech-name {
  position: absolute;
  right: 0;
  bottom: -40rpx;
  color: #ADADAD;
  font-size: 26rpx;
}



